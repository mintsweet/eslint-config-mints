{
  "name": "eslint-config-mints",
  "version": "2.1.0",
  "description": "personal customary rules of a react programmer",
  "keywords": [
    "mints",
    "eslint",
    "config",
    "react",
    "typescript"
  ],
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prettier": "prettier . --write"
  },
  "dependencies": {
    "eslint-config-react-app": "^7.0.1"
  },
  "devDependencies": {
    "husky": "4.3.8",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write"
    ]
  }
}
